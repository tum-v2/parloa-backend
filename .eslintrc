{
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier"],
  "plugins": ["eslint-plugin-tsdoc"],
  "parserOptions": { 
    "ecmaVersion": 12, "sourceType": "module" 
  },
  "rules": { 
    "require-jsdoc": "warn",
    "tsdoc/syntax": "warn"
  }
}
